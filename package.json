{
    "name": "shrine512",
    "version": "1.0.1",
    "main": "dist/shrine512.cjs.js",
    "module": "dist/shrine512.esm.js",
    "browser": "dist/shrine512.umd.js",
    "description": "Convenient and extremely easy to use SHA-512 encryption/decryption of strings or objects with a single method for each direction - easy!",
    "scripts": {
        "example": "node -r esm example/shrine512.example.js",
        "build": "rollup -c",
        "test": "jest",
        "dev": "rollup -c -w && documentation serve --watch src/main.js",
        "docs": "documentation build src/** -f md -o docs/markdown && documentation build src/** -f html -o docs/html",
        "start": "npm run example && npm run test && npm run dev"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bemoje/shrine512"
    },
    "files": [
        "dist"
    ],
    "keywords": [
        "sha512",
        "aes",
        "aes-256",
        "aes-256-gcm",
        "gcm",
        "sha",
        "512",
        "encryption",
        "decryption",
        "shrine",
        "shrine512",
        "encrypt",
        "decrypt",
        "crypt",
        "crypto",
        "easy",
        "simple",
        "beginner",
        "object",
        "string"
    ],
    "author": {
        "name": "Benjamin Mï¿½ller Jensen",
