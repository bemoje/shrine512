    "dependencies": {
        "esm": "^3.2.25"
    },
    "devDependencies": {
        "@types/babel-core": "^6.25.6",
        "@types/jest": "^24.9.1",
        "@types/rollup-plugin-commonjs": "^9.3.1",
        "@types/rollup-plugin-node-resolve": "^4.1.0",
        "babel-core": "^6.26.3",
        "babel-jest": "^23.6.0",
        "babel-preset-env": "^1.7.0",
        "documentation": "^12.1.4",
        "jest": "^24.9.0",
        "math-random": "^1.0.4",
        "randomstring": "^1.1.5",
        "rollup": "^1.29.1",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-node-resolve": "^5.2.0"
    }
